{
  "name": "hrk-overmind",
  "version": "1.0.0",
  "description": "Overmind Tools Suite for Harjun Raskaskone Oy",
  "private": true,
  "scripts": {
    "install:all": "cd server && npm install && cd ../web && npm install",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "cd server && npm run dev",
    "dev:web": "cd web && npm run dev",
    "build": "cd web && npm run build",
    "start": "cd server && npm start",
    "setup": "chmod +x install.sh && ./install.sh"
  },
  "keywords": ["internal", "tools"],
  "author": "Harjun Raskaskone Oy",
  "license": "UNLICENSED",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
